<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="219.0" prefWidth="392.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lapr.ui.javafx.LoginUI">
   <center>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <HBox>
               <children>
                  <Label prefHeight="18.0" prefWidth="67.0" text="Email" />
                  <TextField fx:id="fieldEmail" maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="80.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
            <HBox>
               <children>
                  <Label prefHeight="18.0" prefWidth="67.0" text="Password" />
                  <PasswordField fx:id="fieldPassword" maxWidth="1.7976931348623157E308" minWidth="80.0" prefWidth="80.0" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <bottom>
      <VBox alignment="CENTER" prefHeight="39.0" prefWidth="362.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="bntLogin" mnemonicParsing="false" onAction="#preformLogin" prefHeight="26.0" prefWidth="113.0" text="Login" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </bottom>
   <top>
      <VBox alignment="CENTER" prefHeight="38.0" prefWidth="362.0" BorderPane.alignment="CENTER">
         <children>
            <StackPane prefHeight="34.0" prefWidth="359.0">
               <children>
                  <Label prefHeight="32.0" prefWidth="60.0" text="Login" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="22.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>
         </children>
      </VBox>
   </top>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</BorderPane>
